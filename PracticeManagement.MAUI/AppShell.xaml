<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PracticeManagement.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PracticeManagement.MAUI"
    xmlns:local1="clr-namespace:PracticeManagement.MAUI.Views"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Clients"
        ContentTemplate="{DataTemplate local1:ClientsView}"
        Route="Clients" />
    <ShellContent
        Title="Projects"
        ContentTemplate="{DataTemplate local1:ProjectsView}"
        Route="Projects" />



</Shell>
